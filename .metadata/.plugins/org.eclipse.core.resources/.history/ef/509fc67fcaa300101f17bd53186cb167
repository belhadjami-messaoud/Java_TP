/**
 * 
 */
package app1;

/**
 * 
 */
public class Point {
	
	private Double x;
	private Double y ;
	
	public Point() {
		this.x = 0.0;
		this.y = 0.0;
	}
	public Point(Double x, Double y) {
		this.x = x;
		this.y = y;
	}
	
	public Point(Double x) {
		this.x = x;
	}
	
	public void translate(Double dx,Double dy) {
		this.x= this.x + dx;
		this.y= this.y + dy;
	}

}
